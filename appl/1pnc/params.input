[TimeLoop]
DtInitial = 1e-1 # [s]
TEnd = 1e2 # [s]
MaxTimeStepSize = 2

[Grid]
File = ../1p/grids/10x10-Test-1Pore.dgf
PoreGeometry = Cube
ThroatCrossSectionShape = Square

[Problem]
